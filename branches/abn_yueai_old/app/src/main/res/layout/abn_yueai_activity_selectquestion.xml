<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <include
        layout="@layout/abn_yueai_common_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />

    <TextView
        style="@style/AbnYueAiText.Normal.DeepGray"
        android:layout_margin="@dimen/dim_ailiulian_common_margin_double"
        android:text="@string/abn_yueai_selectquestion_tips"
        />

    <ExpandableListView
        android:id="@+id/expList_selectquestion_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/abn_yueai_text_white"
        android:divider="@mipmap/abn_yueai_list_divider"
        android:dividerHeight="@dimen/dim_ailiulian_common_divider_wide"
        />
</LinearLayout>