<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/iv_splash_change"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@mipmap/abn_ya_login_one" />

    <Button
        android:id="@+id/login_register"
        style="@style/AbnYueAiText.Big.White"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/abn_yueai_appmsg_avatar_size"
        android:background="@mipmap/abn_yueai_ic_register_btnbg"
        android:text="@string/abn_yueai_login" />
</RelativeLayout>