<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_enabled="false">
        <shape>
            <solid android:color="@color/abn_yueai_color_red_primary"/>
            <corners android:radius="99dp"/>
        </shape>
    </item>
    <item android:state_checked="false" android:state_enabled="false">
        <shape>
            <solid android:color="@color/abn_yueai_color_red_primary"/>
            <corners android:radius="99dp"/>
            <stroke
                android:width="1dp"
                android:color="#F0F0F0"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/abn_yueai_color_red_primary"/>
            <corners android:radius="99dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/abn_yueai_text_white_sel"/>
            <corners android:radius="99dp"/>
            <stroke
                android:width="1dp"
                android:color="#E6E6E6"/>
        </shape>
    </item>
</selector>