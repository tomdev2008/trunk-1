package com.aibinong.yueaiapi.pojo;

public class Page {
    public int toPage = 1, totalPage = 1,totalCount;

    public boolean noMore() {
        return toPage >= totalPage;
    }

    public Page() {
        this.toPage = 1;
        this.totalPage = 1;
    }

    public Page(int toPage, int totalPage) {
        this.toPage = toPage;
        this.totalPage = totalPage;
    }
}
