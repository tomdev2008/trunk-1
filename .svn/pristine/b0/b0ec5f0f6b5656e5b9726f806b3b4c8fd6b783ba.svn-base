<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false">

    <android.support.v4.view.ViewPager
        android:id="@+id/pager_guide"
        android:layout_width="fill_parent"
        android:layout_height="match_parent" />

    <com.viewpagerindicator.CirclePageIndicator
        android:id="@+id/indicator_guide"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginBottom="@dimen/dim_ailiulian_common_margin_double"
        android:fitsSystemWindows="true"
        android:padding="2dp"
        app:fillColor="@color/abn_yueai_text_white"
        app:pageColor="@color/abn_yueai_text_white_sel"
        app:radius="@dimen/dim_ailiulian_common_margin_half"
        app:strokeColor="@color/color_common_bordergray"
        app:strokeWidth="1dp" />

</FrameLayout>