            <corners android:radius="@dimen/dim_ailiulian_common_btnheight_small_half" />
            <solid android:color="@color/color_common_translucent_black_10" />
            <stroke android:width="@dimen/dim_ailiulian_common_divider_wide" android:color="@color/abn_yueai_color_red_secondDark" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dim_ailiulian_common_btnheight_small_half" />
