<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/view_common_emptydata_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/iv_common_enptydata_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:src="@mipmap/ic_common_emptydata" />

    <TextView
        android:id="@id/tv_common_emptydata_msg"
        style="@style/AbnYueAiText.Normal.Gray"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/iv_common_enptydata_icon"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dim_ailiulian_common_margin"
        android:gravity="center"
        android:hint="暂时没有任何数据" />

</RelativeLayout>
